{
  "manifest_version": 2,
  "name": "Musescore Downloader",
  "description": "This extension allows you to download sheets and audio.",
  "version": "0.0.1",
  "permissions": [
    "storage",
    "activeTab",
    "webRequest",
    "downloads",
    "https://musescore.com/*",
    "https://s3.ultimate-guitar.com/musescore.scoredata/g/*"
  ],
  "icons": {
    "16": "./src/images/pic16.png",
    "32": "./src/images/pic32.png",
    "48": "./src/images/pic48.png",
    "128": "./src/images/pic128.png"
  },
  "browser_action": {
    "default_popup": "./src/popup.html"
  },
  "background": {
    "scripts": [
      "./src/scripts/background.js"
    ],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; script-src-elem 'self' https://github.com/foliojs/pdfkit/releases/download/v0.12.0/pdfkit.standalone.js https://github-releases.githubusercontent.com/* https://cdn.jsdelivr.net/npm/pdfkit@0.12.1 https://github.com/devongovett/pdfkit/releases/download/v0.6.2/pdfkit.js https://github.com/devongovett/blob-stream/releases/download/v0.1.2/blob-stream-v0.1.2.js https://unpkg.com/pdf-lib https://cdn.jsdelivr.net/npm/svg-to-pdfkit@0.1.8/source.js https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.5/jspdf.debug.js;"
}
